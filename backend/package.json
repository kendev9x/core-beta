{
  "name": "core-beta",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot --config configs/dev.config.js --envfile .run.env services/gw/apis/*.gw.js services/gw/sockets/*.socket.js services/biz/**/start.js",
    "staging": "moleculer-runner --repl --hot --config configs/staging.config.js --envfile .run.env services/gw/apis/*.gw.js services/gw/sockets/*.socket.js services/biz/**/start.js",
    "uat": "moleculer-runner --repl --hot --config configs/uat.config.js --envfile .run.env services/gw/apis/*.gw.js services/gw/sockets/*.socket.js services/biz/**/start.js",
    "prod": "moleculer-runner --repl --hot --config configs/prod.config.js --envfile .run.env services/gw/apis/*.gw.js services/gw/sockets/*.socket.js services/biz/**/start.js",
    "start": "moleculer-runner --envfile .run.env --instances=max bin/services_enable/*.service.js",
    "cli": "moleculer connect NATS",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services/gw",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down",
    "all": "moleculer-runner --repl --config configs/dev.config.js --envfile .run.env bin/services_enable/*.service.js --instances=max",
    "moleculer": "moleculer-runner"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "axios": "^0.21.1",
    "dateformat": "^4.4.1",
    "eslint": "^5.16.0",
    "fastify": "^1.14.6",
    "fastify-static": "^1.1.0",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "json-diff": "^0.5.4",
    "moleculer-repl": "^0.6.4",
    "moleculer-web-swagger": "^0.0.4",
    "request-promise": "^4.2.6",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "swagger-ui-dist": "^3.38.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.10.0",
    "automapper": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "cqrs-domain": "^2.14.79",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "express": "^4.17.1",
    "fastify-swagger": "^3.5.0",
    "http-status-codes": "^2.1.4",
    "i": "^0.3.6",
    "install": "^0.13.0",
    "ioredis": "^4.17.3",
    "jaeger-client": "^3.18.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mkdirp": "^0.5.5",
    "moleculer": "^0.14.14",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongo": "^0.4.7",
    "moleculer-io": "^1.1.3",
    "moleculer-jaeger": "^0.2.3",
    "moleculer-socketio": "0.0.5",
    "moleculer-web": "^0.9.0",
    "mongo-sanitize": "^1.1.0",
    "mongoose": "^5.10.16",
    "mongoose-aggregate-paginate-v2": "^1.0.42",
    "mongoose-field-encryption": "^4.0.1",
    "mongoose-paginate-v2": "^1.3.12",
    "mssql": "^7.1.0",
    "nats": "^1.4.12",
    "npm": "^6.14.13",
    "pm2": "^5.1.0",
    "protobufjs": "^6.10.2",
    "randomatic": "^3.1.1",
    "redis": "^3.1.2",
    "save": "^2.4.0",
    "sharp": "^0.28.3",
    "soap": "^0.39.0",
    "swagger-jsdoc": "^6.0.0-rc.4",
    "swagger-stats": "^0.95.18",
    "swagger-ui-express": "^4.1.5",
    "tedious": "^11.0.8",
    "uuid": "^8.3.2",
    "xlsx-stream-reader": "^1.1.1"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
